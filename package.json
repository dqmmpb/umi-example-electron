{
  "name": "umi-example-electron",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "dengqiming",
    "email": "dqmmpb@163.com"
  },
  "scripts": {
    "build:web": "APP_ROOT=src/renderer ESLINT=none umi build",
    "clean": "rm -rf ./build && rm -fr ./dist",
    "compile:electron": "rm -rf ./dist && npm run compile:electron:renderer && npm run compile:electron:main",
    "compile:electron:main": "ESLINT=none electron-webpack",
    "compile:electron:renderer": "APP_ROOT=src/renderer ESLINT=none UMI_ENV=electron umi build",
    "dev:electron": "concurrently \"npm run dev:electron:renderer\" \"npm run dev:electron:main\"",
    "dev:electron:main": "ESLINT=none BROWSER=none UMI_ENV=electron electron-webpack dev",
    "dev:electron:renderer": "APP_ROOT=src/renderer BROWSER=none ESLINT=none UMI_ENV=electron umi dev",
    "pack:electron": "rm -rf ./build/electron && yarn compile:electron && electron-builder",
    "pack:electron:dir": "rm -rf ./build/electron && yarn compile:electron && electron-builder --dir",
    "start": "APP_ROOT=src/renderer BROWSER=none ESLINT=none umi dev"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "concurrently": "^3.5.1",
    "electron": "^5.0.6",
    "electron-builder": "^21.0.11",
    "electron-debug": "^2.0.0",
    "electron-is": "^3.0.0",
    "electron-log": "^3.0.8",
    "electron-rebuild": "^1.7.3",
    "electron-store": "^2.0.0",
    "electron-webpack": "^2.7.4",
    "slash": "^2.0.0",
    "umi": "^2.9.6",
    "umi-build-dev": "^1.12.0",
    "umi-plugin-dva": "^1.8.0",
    "umi-plugin-react": "^1.10.1",
    "webpack": "^4.38.0",
    "webpack-dev-server": "^3.7.2"
  },
  "build": {
    "files": [
      "./dist/**/*",
      "./node_modules/**/*"
    ],
    "appId": "com.izaiqi.umi-example-electron",
    "productName": "umi-example-electron",
    "copyright": "Copyright @ 2019",
    "asar": false,
    "directories": {
      "output": "./build/electron"
    },
    "mac": {
      "target": "tar.gz"
    }
  }
}
